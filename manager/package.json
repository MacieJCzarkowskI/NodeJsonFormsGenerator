{
  "name": "manager",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test:coverage": "npm test --coverage",
    "test:debug": "npm test --debug --detectOpenHandles",
    "test:ci": "npm test --runInBand --no-cache --ci",
    "test": "NODE_ENV=test jest",
    "lint": "eslint ./{src,pages}/**/*",
    "format": "eslint './{src,pages}/**/*' --ext .js,.jsx,.tsx,.ts --fix",
    "analyze": "cross-env BUNDLE_ANALYZE=both next build",
    "export": "cross-env NODE_ENV=production next export",
    "production": "npm-run-all analyze export",
    "production:clean": "rimraf out/",
    "lint:fix": "tslint --fix --project .",
    "prepare": "yarn link common-ui"
  },
  "dependencies": {
    "@emotion/react": "11.1.1",
    "@emotion/styled": "10.0.27",
    "@reduxjs/toolkit": "1.4.0",
    "@tippyjs/react": "4.2.0",
    "classnames": "2.2.6",
    "cssnano": "4.1.10",
    "date-fns": "2.16.1",
    "emotion": "10.0.27",
    "nanoid": "3.1.16",
    "next": "10.0.2",
    "ramda": "0.27.1",
    "react": "17.0.1",
    "react-dom": "16.13.1",
    "react-hook-form": "6.9.4",
    "react-icons": "^4.4.0",
    "react-redux": "7.2.1",
    "react-select": "3.1.0",
    "redux": "4.0.5",
    "tailwindcss": "2.0.1",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/preset-react": "7.12.5",
    "@babel/preset-typescript": "7.12.1",
    "@babel/runtime": "7.12.1",
    "@emotion/babel-preset-css-prop": "10.0.27",
    "@testing-library/jest-dom": "5.11.5",
    "@testing-library/react": "11.1.1",
    "@testing-library/react-hooks": "3.4.2",
    "@testing-library/user-event": "7.1.2",
    "@types/classnames": "2.2.10",
    "@types/jest": "26.0.15",
    "@types/node": "14.11.8",
    "@types/ramda": "0.27.32",
    "@types/react": "16.9.51",
    "@types/react-addons-test-utils": "0.14.25",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/react-select": "3.0.22",
    "@types/react-test-renderer": "16.9.3",
    "@types/redux": "3.6.0",
    "@types/yup": "0.29.8",
    "@typescript-eslint/eslint-plugin": "4.4.0",
    "@typescript-eslint/parser": "4.4.0",
    "autoprefixer": "9.8.6",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-emotion": "10.0.33",
    "babel-plugin-inline-svg": "1.2.0",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-react-css-modules": "5.2.6",
    "cross-env": "7.0.2",
    "dotenv": "8.2.0",
    "eslint": "7.11.0",
    "eslint-config-airbnb-typescript": "11.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-import-resolver-babel-module": "5.1.2",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.4",
    "eslint-plugin-react-hooks": "4.1.2",
    "husky": "3.0.2",
    "jest": "26.6.3",
    "lint-staged": "9.2.1",
    "next-transpile-modules": "4.1.0",
    "npm-link-shared": "0.5.6",
    "npm-run-all": "4.1.5",
    "postcss": "8.1.7",
    "postcss-import": "12.0.1",
    "prettier": "2.1.2",
    "react-addons-test-utils": "15.6.2",
    "react-is": "16.13.1",
    "rimraf": "3.0.2",
    "svg-url-loader": "6.0.0",
    "ts-jest": "26.4.0",
    "tsconfig-paths-webpack-plugin": "3.3.0",
    "typescript": "4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint .",
      "eslint --fix",
      "prettier --config ./prettier.config.js --write",
      "git add"
    ],
    "pages/**/*.{ts,tsx,js,jsx}": [
      "eslint .",
      "eslint --fix",
      "prettier --config ./prettier.config.js --write",
      "git add"
    ]
  }
}
